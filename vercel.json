{
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "no-store" }
      ]
    }
  ],
  "redirects": [
    { "source": "/", "destination": "/landing.html", "permanent": false }
  ],
  "crons": [
    {
      "path": "/api/cron-transcribe-pending",
      "schedule": "*/5 * * * *"
    }
  ]
}
